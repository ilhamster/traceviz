{
  "name": "@traceviz/client-core",
  "version": "1.0.1",
  "description": "A platform for building highly-reusable performance tools.",
  "type": "module",
  "main": "./dist/core.js",
  "types": "./dist/core.d.ts",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/core.js",
      "types": "./dist/core.d.ts",
      "default": "./dist/core.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc --declaration",
    "test": "node --loader ts-node/esm ./node_modules/jasmine/bin/jasmine.js --config=spec/support/jasmine.js",
    "bt": "npm run build && npm run test",
    "cbt": "npm run clean && npm run bt"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ilhamster/traceviz.git",
    "directory": "client/core"
  },
  "author": "Lee Baugh",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/d3-dispatch": "3.0.2",
    "@types/jasmine": "^4.6.5",
    "@types/node": "^18.19.130",
    "@types/pako": "^2.0.4",
    "jasmine": "^4.6.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts-console-reporter": "^3.1.1",
    "rimraf": "^4.4.1",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "d3": "^7.0.0",
    "rxjs": "~7.8.0"
  },
  "dependencies": {
    "pako": "^2.1.0",
    "buffer": "^6.0.3"
  }
}
